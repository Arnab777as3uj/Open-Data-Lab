{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::<<account-12digitid>>:user/<<iamuser>>"
            },
            "Action": "s3:*",
            "Resource": "arn:aws:s3:::<<bucket-name>>/*"
        }
    ]
}
